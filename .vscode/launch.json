{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Code Push Server",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v22.6.0/bin/node",
      "env": {
        "APP_PORT": 5003,
        "ZK_URL": "zk.du2jrq.private.xyz:2181",
        "ZK_ROOT": "FE",
        "NODE_ENV": "development",
        "LOGGING": "true",
        "DEBUG_DISABLE_AUTH": "true",
        "DEBUG_USER_ID": 123,
        "REDIS_HOST": "localhost",
        "REDIS_PORT": 6379,
        "REDIS_KEY": "your_redis_key"
      },
      "program": "${workspaceFolder}/api/bin/script/server.js",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Node",
      "processId": "${command:PickProcess}",
      "restart": true
    }
  ]
}
